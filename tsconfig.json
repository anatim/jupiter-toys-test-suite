{
  "compilerOptions": {
    "target": "ES6",
    "lib": ["es6", "dom"],
    "types": ["cypress", "node", "mocha", "chai"],
    "moduleResolution": "node",
    "esModuleInterop": true,
    "allowJs": true,
    "outDir": "./dist",
    "strict": true,
    "skipLibCheck": true,
    "baseUrl": "http://jupiter.cloud.planittesting.com"
  },
  "include": [
    "cypress/**/*.ts",
    "cypress/**/*.d.ts",
    "node_modules/cypress/types/index.d.ts",
  ]
}
